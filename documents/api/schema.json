{
    "openapi": "3.0.2",
    "info": {
        "title": "sns-app",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http:\/\/localhost"
        }
    ],
    "paths": {
        "\/api\/v1\/users\/me": {
            "get": {
                "summary": "ログイン済みかどうかの判定用エンドポイント",
                "responses": {
                    "200": {
                        "description": "Check who am I",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/UserResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "\/api\/v1\/users": {
            "post": {
                "summary": "ユーザ登録用エンドポイント",
                "requestBody": {
                    "description": "Signup Request Body",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/SignupRequestBody"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Signup Successful",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/UserResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/BadRequestResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "\/api\/v1\/users\/{userId}": {
            "put": {
                "summary": "ユーザ情報更新用エンドポイント",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": " Update user info Request Body",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/UpdateUserRequestBody"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Update Successful",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/UserResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/BadRequestResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "\/api\/v1\/users\/{userId}\/password": {
            "put": {
                "summary": "パスワード更新用エンドポイント",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Update user password Request Body",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/UpdateUserPasswordRequestBody"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Password Update Successful"
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/BadRequestResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "\/api\/v1\/login": {
            "post": {
                "summary": "ログイン用エンドポイント",
                "requestBody": {
                    "description": "Login Request Body",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/LoginRequestBody"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Login Successful",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/UserResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/BadRequestResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "\/api\/v1\/logout": {
            "post": {
                "summary": "ログアウト用エンドポイント",
                "responses": {
                    "200": {
                        "description": "Logout Successful"
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/BadRequestResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "\/api\/v1\/messages": {
            "post": {
                "summary": "メッセージ投稿用エンドポイント",
                "requestBody": {
                    "description": "Message Create Request Body",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/MessageCreateBody"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Message Create Successful"
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/BadRequestResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            },
            "get": {
                "summary": "メッセージ取得用エンドポイント",
                "requestBody": {
                    "description": "Message Create Request Body",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/MessageCreateBody"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Get Message List",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/MessageListResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/BadRequestResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "\/api\/v1\/messages\/{messageId}\/favorite": {
            "put": {
                "summary": "お気に入り登録用エンドポイント",
                "parameters": [
                    {
                        "name": "messageId",
                        "in": "path",
                        "description": "",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": "FavoriteReverse Request Body",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/FavoriteReverseRequestBody"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "FavoriteReverse Successful"
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/BadRequestResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "BadRequestResponse": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string",
                        "nullable": true
                    }
                }
            },
            "FavoriteReverseRequestBody": {
                "type": "object",
                "required": [
                    "isFavorite"
                ],
                "properties": {
                    "isFavorite": {
                        "type": "boolean"
                    }
                }
            },
            "LoginRequestBody": {
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                }
            },
            "MessageCreateBody": {
                "type": "object",
                "required": [
                    "body"
                ],
                "properties": {
                    "body": {
                        "type": "string"
                    }
                }
            },
            "MessageListRequestBody": {
                "type": "object",
                "properties": {
                    "perPage": {
                        "type": "integer"
                    },
                    "lastMessageId": {
                        "type": "string"
                    }
                }
            },
            "MessageListResponse": {
                "type": "array",
                "items": {
                    "type": "object",
                    "required": [
                        "id",
                        "user_id",
                        "body",
                        "created_at",
                        "isFavorite",
                        "favoritesCount"
                    ],
                    "properties": {
                        "id": {
                            "type": "string"
                        },
                        "user_id": {
                            "type": "string"
                        },
                        "body": {
                            "type": "string"
                        },
                        "created_at": {
                            "type": "string"
                        },
                        "isFavorite": {
                            "type": "boolean"
                        },
                        "favoritesCount": {
                            "type": "integer"
                        }
                    }
                }
            },
            "NotFoundResponse": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string",
                        "nullable": true
                    }
                }
            },
            "SignupRequestBody": {
                "type": "object",
                "required": [
                    "name",
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                }
            },
            "UpdateUserPasswordRequestBody": {
                "type": "object",
                "required": [
                    "password"
                ],
                "properties": {
                    "password": {
                        "type": "string"
                    }
                }
            },
            "UpdateUserRequestBody": {
                "type": "object",
                "required": [
                    "name",
                    "email"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    }
                }
            },
            "UserResponse": {
                "type": "object",
                "required": [
                    "id",
                    "name",
                    "email"
                ],
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
